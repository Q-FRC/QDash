#ifndef WIN32_LEAN_AND_MEAN
#define WIN32_LEAN_AND_MEAN
#endif
#include <windows.h>

IDI_ICON1               ICON    DISCARDABLE     "QDash.ico"
10255 RT_MANIFEST "QDash.manifest"

VS_VERSION_INFO VERSIONINFO
FILEVERSION     @QDASH_NUMERIC_TAG@
FILEOS          VOS_NT_WINDOWS32
FILETYPE        VFT_APP
BEGIN
        BLOCK "StringFileInfo"
        BEGIN
                BLOCK "000004b0"
                BEGIN
                        VALUE "CompanyName", "@QDASH_ORG@"
                        VALUE "FileDescription", "QDash"
                        VALUE "FileVersion", "@QDASH_TAG@"
                        VALUE "ProductName", "QDash"
                        VALUE "ProductVersion", "@GIT_TAG@"
                END
        END
        BLOCK "VarFileInfo"
        BEGIN
                VALUE "Translation", 0x0000, 0x04b0 // Unicode
        END
END
